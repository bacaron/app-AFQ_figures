#!/bin/bash
#PBS -l nodes=1:ppn=16:dc2,walltime=0:10:00
#PBS -N app-LIFE_AFQ_qualitycheck
#PBS -V

if [ $ENV == "IUHPC" ]; then
	cd $PBS_O_WORKDIR
	module load matlab
fi

export MATLABPATH=$MATLABPATH:$SERVICE_DIR
matlab -nodisplay -nosplash -r main

if [ -s fe_AFQ_StatPlot.png ];
then 
  echo 0 > finished
else
  echo "plot missing"
  echo 1 > finished
  exit 1
fi
